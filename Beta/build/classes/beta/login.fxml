      <TextField fx:id="username" layoutX="261.0" layoutY="185.0" prefHeight="27.0" prefWidth="213.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="261.0" layoutY="252.0" prefHeight="27.0" prefWidth="213.0" promptText="password" />
      <ImageView fitHeight="16.0" fitWidth="16.0" layoutX="231.0" layoutY="191.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/person%20-black.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="16.0" fitWidth="19.0" layoutX="231.0" layoutY="258.0" pickOnBounds="true" preserveRatio="true">
